{
  "manifest_version": 3,
  "name": "LeetCode Complexity Checker",
  "version": "1.0",
  "description": "Checks the estimated time complexity of LeetCode editor code.",
  "permissions": ["scripting", "activeTab", "storage"],
  "host_permissions": [
  "http://127.0.0.1/*",
  "http://localhost/*",
  "https://leetcode.com/*"
],
  "action": {
    "default_popup": "popup.html"
  } 
}
